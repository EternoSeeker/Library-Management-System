<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarydbnew.LoginCheckController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" layoutX="125.0" layoutY="119.0" text="Library Management System" textFill="#002966" translateY="-50.0">
      <font>
         <Font name="System Bold" size="26.0" />
      </font></Label>
   <HBox />
   <HBox />
   <Button alignment="CENTER" contentDisplay="CENTER" layoutX="167.0" layoutY="163.0" mnemonicParsing="false" onAction="#switchAdmin" prefHeight="40.0" prefWidth="90.0" text="Admin">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Button layoutX="303.0" layoutY="253.0" mnemonicParsing="false" onAction="#switchSignUp" text="Sign Up" textFill="#001d5b" underline="true">
      <font>
         <Font size="14.0" />
      </font>
   </Button>
   <Button alignment="CENTER" contentDisplay="CENTER" layoutX="337.0" layoutY="163.0" mnemonicParsing="false" onAction="#switchMember" prefHeight="40.0" prefWidth="90.0" text="User">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Label alignment="CENTER" contentDisplay="CENTER" layoutX="203.0" layoutY="257.0" text="New User ?" textFill="#00297b">
      <font>
         <Font size="15.0" />
      </font>
   </Label>
</AnchorPane>
